    <servlet-name>controller</servlet-name>
    <url-pattern>/controller</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PersonalController</servlet-name>
    <servlet-class>controller.PersonalTickerController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PersonalController</servlet-name>
    <url-pattern>/personalcontroller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PortFolioController</servlet-name>
